&#xA;&#x9;&#x9;&#x9;&#x9;&#x9;CHARINDEX(''&lt;/Value&gt;'', REPLACE(SUBSTRING(ExtensionSettings, CHARINDEX(''&lt;Name&gt;TO&lt;/Name&gt;&lt;Value&gt;'',ExtensionSettings), 10000), ''&lt;Name&gt;TO&lt;/Name&gt;&lt;Value&gt;'', ''''))-1&#xA;&#x9;&#x9;&#x9;&#x9;)  not like ''&lt;Parameter%''&#xA;&#xA;&#x9;&#x9;&#x9;&#x9;union all&#xA;&#x9;-- epcog&#xA;&#xA;&#x9;SELECT [SubscriptionID]&#xA;&#x9;&#x9;  ,[Report_OID]&#xA;&#x9;&#x9;  ,[InactiveFlags]&#xA;&#x9;&#x9;  ,LEFT(&#xA;&#x9;&#x9;&#x9;&#x9;&#x9;REPLACE(SUBSTRING(ExtensionSettings, CHARINDEX(''&lt;Name&gt;TO&lt;/Name&gt;&lt;Value&gt;'',ExtensionSettings), 10000), ''&lt;Name&gt;TO&lt;/Name&gt;&lt;Value&gt;'', ''''), &#xA;&#x9;&#x9;&#x9;&#x9;&#x9;CHARINDEX(''&lt;/Value&gt;'', REPLACE(SUBSTRING(ExtensionSettings, CHARINDEX(''&lt;Name&gt;TO&lt;/Name&gt;&lt;Value&gt;'',ExtensionSettings), 10000), ''&lt;Name&gt;TO&lt;/Name&gt;&lt;Value&gt;'', ''''))-1&#xA;&#x9;&#x9;&#x9;&#x9;) AS EmailList&#xA;&#x9;&#x9;  ,[Description]&#xA;&#x9;&#x9;  ,[LastStatus]&#xA;&#x9;&#x9;  ,[LastRunTime]&#xA;&#x9;&#x9;  ,LTRIM(RTRIM(s.Value)) SubscriptionTo&#xA;&#x9;FROM ' + @DG_STAGE_DB + '.[raw].[clarity_server-reportserver-dbo-subscriptions] Subscriptions&#xA;&#x9;  CROSS APPLY STRING_SPLIT(&#xA;&#x9;&#x9;&#x9;&#x9;&#x9;&#x9;&#x9;&#x9;LEFT(&#xA;&#x9;&#x9;&#x9;&#x9;&#x9;&#x9;&#x9;&#x9;&#x9;&#x9;REPLACE(SUBSTRING(ExtensionSettings, CHARINDEX(''&lt;Name&gt;TO&lt;/Name&gt;&lt;Value&gt;'',ExtensionSettings), 10000), ''&lt;Name&gt;TO&lt;/Name&gt;&lt;Value&gt;'', ''''), &#xA;&#x9;&#x9;&#x9;&#x9;&#x9;&#x9;&#x9;&#x9;&#x9;&#x9;CHARINDEX(''&lt;/Value&gt;'', REPLACE(SUBSTRING(ExtensionSettings, CHARINDEX(''&lt;Name&gt;TO&lt;/Name&gt;&lt;Value&gt;'',ExtensionSettings), 10000), ''&lt;Name&gt;TO&lt;/Name&gt;&lt;Value&gt;'', ''''))-1&#xA;&#x9;&#x9;&#x9;&#x9;&#x9;&#x9;&#x9;&#x9;&#x9;), '';'') as s&#xA;&#xA;&#x9;WHERE Subscriptions.ExtensionSettings LIKE ''%;%'' &#xA;&#x9;AND s.value IS NOT NULL and s.value != ''''&#xA;&#xA;&#x9;/**************Reports that dont go to multiple people, or go to a path********************/&#xA;&#xA;&#x9;UNION&#xA;&#xA;&#x9;SELECT [SubscriptionID]&#xA;&#x9;&#x9;  ,[Report_OID]&#xA;&#x9;&#x9;  ,[InactiveFlags]&#xA;&#xA;&#x9;&#x9;  ,LEFT(&#xA;&#x9;&#x9;&#x9;&#x9;&#x9;REPLACE(SUBSTRING(ExtensionSettings, CHARINDEX(''&lt;Name&gt;TO&lt;/Name&gt;&lt;Value&gt;'',ExtensionSettings), 10000), ''&lt;Name&gt;TO&lt;/Name&gt;&lt;Value&gt;'', ''''), &#xA;&#x9;&#x9;&#x9;&#x9;&#x9;CHARINDEX(''&lt;/Value&gt;'', REPLACE(SUBSTRING(ExtensionSettings, CHARINDEX(''&lt;Name&gt;TO&lt;/Name&gt;&lt;Value&gt;'',ExtensionSettings), 10000), ''&lt;Name&gt;TO&lt;/Name&gt;&lt;Value&gt;'', ''''))-1&#xA;&#x9;&#x9;&#x9;&#x9;) AS EmailList&#xA;&#x9;&#x9;  ,[Description]&#xA;&#x9;&#x9;  ,[LastStatus]&#xA;&#x9;&#x9;  ,[LastRunTime]&#xA;&#x9;&#x9;  ,REPLACE(LEFT(&#xA;&#x9;&#x9;&#x9;&#x9;&#x9;&#x9;REPLACE(SUBSTRING(ExtensionSettings, CHARINDEX(''&lt;Name&gt;TO&lt;/Name&gt;&lt;Value&gt;'',ExtensionSettings), 10000), ''&lt;Name&gt;TO&lt;/Name&gt;&lt;Value&gt;'', ''''), &#xA;&#x9;&#x9;&#x9;&#x9;&#x9;&#x9;CHARINDEX(''&lt;/Value&gt;'', REPLACE(SUBSTRING(ExtensionSettings, CHARINDEX(''&lt;Name&gt;TO&lt;/Name&gt;&lt;Value&gt;'',ExtensionSettings), 10000), ''&lt;Name&gt;TO&lt;/Name&gt;&lt;Value&gt;'', ''''))-1&#xA;&#x9;&#x9;&#x9;&#x9;&#x9;&#x9;), ''&lt;ParameterValues&gt;&lt;ParameterValue&gt;&lt;Name&gt;PATH&lt;/Name&gt;&lt;Value&gt;'', '''')&#xA;&#xA;&#x9;FROM ' + @DG_STAGE_DB + '.[raw].[clarity_server-reportserver-dbo-subscriptions] Subscriptions&#xA;&#xA;&#x9;WHERE Subscriptions.ExtensionSettings NOT LIKE ''%;%'' &#xA;&#x9; and LEFT(&#xA;&#x9;&#x9;&#x9;&#x9;&#x9;REPLACE(SUBSTRING(ExtensionSettings, CHARINDEX(''&lt;Name&gt;TO&lt;/Name&gt;&lt;Value&gt;'',ExtensionSettings), 10000), ''&lt;Name&gt;TO&lt;/Name&gt;&lt;Value&gt;'', ''''), &#xA;&#x9;&#x9;&#x9;&#x9;&#x9;CHARINDEX(''&lt;/Value&gt;'', REPLACE(SUBSTRING(ExtensionSettings, CHARINDEX(''&lt;Name&gt;TO&lt;/Name&gt;&lt;Value&gt;'',ExtensionSettings), 10000), ''&lt;Name&gt;TO&lt;/Name&gt;&lt;Value&gt;'', ''''))-1&#xA;&#x9;&#x9;&#x9;&#x9;)  not like ''&lt;Parameter%''&#xA;&#x9;&#x9;&#x9;&#x9;) S &#xA;&#xA;&#x9;left join ' + @DG_DB + '.[dbo].ReportObject r on s.Report_OID = r.ReportServerCatalogId&#xA;&#x9;left outer join ' + @DG_DB + '.dbo.[User] u on REPLACE(s.SubscriptionTo,''' + @ORG_AD_NAME + ''',''rhc'') = u.Email&#xA;) as s&#xA;on (t.ReportObjectId = s.reportobjectId&#xA;&#x9;and isnull(t.userId,999999) = isnull(s.userid,999999)&#xA;&#x9;and s.subscriptionid = t.subscriptionid&#xA;&#x9;and s.inactiveflags = t.inactiveflags&#xA;&#x9;and s.emaillist = t.emaillist&#xA;&#x9;and s.description = t.description&#xA;&#x9;and s.subscriptionto = t.subscriptionto)&#xA;when matched and (s.laststatus != t.laststatus or s.lastruntime != t.lastruntime)&#xA;&#x9;then update&#xA;&#x9;&#x9;set t.laststatus = s.laststatus, &#xA;&#x9;&#x9;    t.lastruntime = s.lastruntime,&#xA;&#x9;&#x9;&#x9;t.lastloaddate = GetDate()&#xA;When not matched by target&#xA;&#x9;then insert (&#xA;&#x9;&#x9;&#x9;&#x9;&#x9;[ReportObjectID]&#xA;&#x9;&#x9;&#x9;&#x9;&#x9;,[UserId]&#xA;&#x9;&#x9;&#x9;&#x9;&#x9;,[SubscriptionId]&#xA;&#x9;&#x9;&#x9;&#x9;&#x9;,[InactiveFlags]&#xA;&#x9;&#x9;&#x9;&#x9;&#x9;,[EmailList]&#xA;&#x9;&#x9;&#x9;&#x9;&#x9;,[Description]&#xA;&#x9;&#x9;&#x9;&#x9;&#x9;,[LastStatus]&#xA;&#x9;&#x9;&#x9;&#x9;&#x9;,[LastRunTime]&#xA;&#x9;&#x9;&#x9;&#x9;&#x9;,[SubscriptionTo]&#xA;&#x9;&#x9;&#x9;&#x9;&#x9;,[LastLoadDate]&#xA;&#x9;&#x9;&#x9;&#x9;)&#xA;&#x9;&#x9; values (s.reportobjectid&#xA;&#x9;&#x9;&#x9;&#x9;,s.userId&#xA;&#x9;&#x9;&#x9;&#x9;,s.subscriptionid&#xA;&#x9;&#x9;&#x9;&#x9;,s.inactiveflags&#xA;&#x9;&#x9;&#x9;&#x9;,s.emaillist&#xA;&#x9;&#x9;&#x9;&#x9;,s.description&#xA;&#x9;&#x9;&#x9;&#x9;,s.laststatus&#xA;&#x9;&#x9;&#x9;&#x9;,s.lastruntime&#xA;&#x9;&#x9;&#x9;&#x9;,s.subscriptionto&#xA;&#x9;&#x9;&#x9;&#x9;,GetDate())&#xA;when not matched by source&#xA;&#x9;then delete;&#xA;')" xmlns:SQLTask="www.microsoft.com/sqlserver/dts/tasks/sqltask">
