left outer join raw.[clarity_server-clarityreport-dbo-emp_basic_info]    puser2 on puser2.USER_ID = puser.USER_ID
left outer join raw.[clarity_server-clarityreport-dbo-dashboard_info]    idm_override on idm_override.OVRIDE_PARENT_DB_ID = idm.DASHBOARD_ID
