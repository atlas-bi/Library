              <properties>
                <property
                  dataType="System.Int32"
                  description="The number of seconds before a command times out.  A value of 0 indicates an infinite time-out."
                  name="CommandTimeout">0</property>
                <property
                  dataType="System.String"
                  description="Specifies the name of the database object used to open a rowset."
                  name="OpenRowset"></property>
                <property
                  dataType="System.String"
                  description="Specifies the variable that contains the name of the database object used to open a rowset."
                  name="OpenRowsetVariable"></property>
                <property
                  dataType="System.String"
                  description="The SQL command to be executed."
                  name="SqlCommand"
                  UITypeEditor="Microsoft.DataTransformationServices.Controls.ModalMultilineStringEditor, Microsoft.DataTransformationServices.Controls, Version=15.0.0.0, Culture=neutral, PublicKeyToken=89845dcd8080cc91">-- HGR report template
select          N'clarity_server'                           as SourceServer
,               N'clarityreport'                        as SourceDB
,               N'template_info'                        as SourceTable
,               convert(nvarchar(max), hgr.REPORT_NAME) as Name
,               convert(nvarchar(max), td.DESCRIPTION)  as Description
-- Description table has multiple lines per HGR. We must use XML to concatenate them.
,               ltrim(stuff((
                                select   N' ' + td.SEARCH_SOURCE_DESC
                                from     raw.[clarity_server-clarityreport-dbo-template_description] td
                                where    td.REPORT_ID = hgr.REPORT_ID
                                order by td.REPORT_ID
                                ,        td.LINE
                                for xml path('')
                            )
                      ,     1
                      ,     1
                      ,     ''
                      )
                )                                       as [DetailedDescription]
,               case
                    when rec_type.NAME = 'Settings' then N'Application Report Template'
                    when rec_type.NAME = 'Workbench' then N'Reporting Workbench Template'
                    when rec_type.NAME is null then N'Other HGR Template'
                    else concat(rec_type.NAME, N' Template')
                end                                     as ReportObjectType
,               null                                    as Author
,               null                                    as LastModifiedBy
,               null                                    as LastModifiedDate
,               null                                    as ReportObjectURL
,               N'HGR'                                  as EpicMasterFile
,               hgr.REPORT_ID                           as EpicRecordID
,               null                                    as ParentURL
,               N'HGR'                                  as ParentMasterFile
,               hgr.SRC_TEMPL_ID                        as ParentRecordID
,       null                  as ReportServerCatalogID
,       case 
          when f.TEMP_ID is null--Not living on a zzz-untested report template
              and (STATUS_C = 0 or STATUS_C is null) --status is active or null
            then N'Y'
          else N'N'
        end                   as DefaultVisibilityYN

from            raw.[clarity_server-clarityreport-dbo-template_info]      hgr
left outer join raw.[clarity_server-clarityreport-dbo-zc_report_type_hgr] rec_type on rec_type.REPORT_TYPE_HGR_C = hgr.REPORT_TYPE_HGR_C
-- for each REPORT_ID in TEMPLATE_DYNAMIC there can be many contacts. To get only the latest contact, 
-- grab the highest CONTACT_NUM for each REPORT_ID, and then use that for the join to TEMPLATE_DYNAMIC
left outer join (
    select   REPORT_ID
    ,        max(CONTACT_NUM) as CONTACT_NUM
    from     raw.[clarity_server-clarityreport-dbo-template_dynamic]
    group by REPORT_ID
)                                                                     tdc on tdc.REPORT_ID = hgr.REPORT_ID
left outer join raw.[clarity_server-clarityreport-dbo-template_dynamic]   td on td.REPORT_ID = tdc.REPORT_ID
                                                                            and td.CONTACT_NUM = tdc.CONTACT_NUM

left outer join 
  (
    select distinct ASSOC_REPORT_ID TEMP_ID 
    from raw.[clarity_server-clarityreport-dbo-clarity_rpt_groups] g 
      inner join raw.[clarity_server-clarityreport-dbo-clarity_rpt] rpt on rpt.REPORT_ID = g.REPORT_ID 
    where REPORT_GROUP_C = '308000001' --ZZZ-untested
  ) f on f.TEMP_ID = hgr.REPORT_ID</property>
