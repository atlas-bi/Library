,               concat(coalesce(idbt.NAME, N'Other'), N' Radar Dashboard Component') as ReportObjectType
,               case when puser.domainname is null then puser.name else concat(N'',?,'\', puser.domainname) end                      as Author

,               null                                                                 as LastModifiedBy
,               idb.INSTANT_OF_UPD_DTTM                                              as LastModifiedDate
,              null as ReportObjectURL
,               N'IDB'                                                               as EpicMasterFile
,               idb.COMPONENT_ID                                                     as EpicRecordID
,       null                                 as ReportServerCatalogID
,       case
          when (RECORD_STATUS_C is null or RECORD_STATUS_C = 0) --Not hidden/deleted/inactivated/etc
              and RECORD_TYPE_C &lt;&gt; 3 --Personal
              and READY_FOR_USE_YN = 'Y'
