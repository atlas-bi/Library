    <DTS:Executable
      DTS:refId="Package\Load User table"
      DTS:CreationName="Microsoft.ExecuteSQLTask"
      DTS:Description="Execute SQL Task"
      DTS:DTSID="{87BCC346-E199-4AE9-8096-FD1BA2BFC4EC}"
      DTS:ExecutableType="Microsoft.ExecuteSQLTask"
      DTS:LocaleID="-1"
      DTS:ObjectName="Load User table"
      DTS:ThreadHint="0">
      <DTS:Variables />
      <DTS:ObjectData>
        <SQLTask:SqlTaskData
          SQLTask:Connection="{6C78DD55-6FB2-44EC-BCBF-01010FE5BF03}"
          SQLTask:BypassPrepare="False"
          SQLTask:SqlStatementSource="DECLARE @SQL nvarchar(max);&#xA;DECLARE @DG_DB VARCHAR(100);&#xA;DECLARE @DG_STAGE_DB VARCHAR(100);&#xA;DECLARE @ORG_AD_NAME VARCHAR(100);&#xA;SELECT @DG_DB =?;&#xA;SELECT @DG_STAGE_DB =?;&#xA;SELECT @ORG_AD_NAME = ?;&#xA;&#xA;&#xA;&#xA;/*&#xA;&#x9;users come from 3 places.&#xA;&#x9;1. import active users from LDAP&#xA;&#x9;2. SSRS user staging - unconfirmed but hope this has x employees history still.&#xA;&#x9;3. Epic user staging - has all employees regardless of status. Any with a domain account who have &quot;logged in&quot; to hyperspace have a domain name&#xA;&#xA;&#x9;Finally, if an epic user has not domain account specified we will import the epic user name in order to have a complete dataset for &#xA;&#x9;report history.&#xA;&#xA;&#x9;Import user groups from epic.&#xA;&#xA;*/&#xA;&#xA;-- LDAP user information&#xA;select @sql = '&#xA;MERGE ' + @DG_DB + '.dbo.[User] dgu USING (select * from LDAP.dbo.Users ldapu where AccountName is not null and AccountName != '''') ldapu&#xA;ON lower(dgu.Username) = lower(ldapu.AccountName)&#xA;WHEN MATCHED and (dgu.EmployeeID != ldapu.EmployeeID&#xA;&#x9;&#x9;&#x9;&#x9;or dgu.AccountName != ldapu.AccountName&#xA;&#x9;&#x9;&#x9;&#x9;or dgu.DisplayName != ldapu.DisplayName&#xA;&#x9;&#x9;&#x9;&#x9;or dgu.FullName != ldapu.FullName&#xA;&#x9;&#x9;&#x9;&#x9;or dgu.FirstName != ldapu.FirstName&#xA;&#x9;&#x9;&#x9;&#x9;or dgu.LastName != ldapu.LastName&#xA;&#x9;&#x9;&#x9;&#x9;or dgu.Department != ldapu.Department&#xA;&#x9;&#x9;&#x9;&#x9;or dgu.Title != ldapu.Title&#xA;&#x9;&#x9;&#x9;&#x9;or dgu.Phone != ldapu.Phone&#xA;&#x9;&#x9;&#x9;&#x9;or dgu.Email != ldapu.Email&#xA;&#x9;&#x9;&#x9;&#x9;or dgu.Base != ldapu.Base)&#xA;    THEN update set&#xA;                dgu.EmployeeID = ldapu.EmployeeID,&#xA;&#x9;&#x9;&#x9;&#x9;dgu.AccountName = ldapu.AccountName,&#xA;&#x9;&#x9;&#x9;&#x9;dgu.DisplayName = ldapu.DisplayName,&#xA;&#x9;&#x9;&#x9;&#x9;dgu.FullName = ldapu.FullName,&#xA;&#x9;&#x9;&#x9;&#x9;dgu.FirstName = ldapu.FirstName,&#xA;&#x9;&#x9;&#x9;&#x9;dgu.LastName = ldapu.LastName,&#xA;&#x9;&#x9;&#x9;&#x9;dgu.Department = ldapu.Department,&#xA;&#x9;&#x9;&#x9;&#x9;dgu.Title = ldapu.Title,&#xA;&#x9;&#x9;&#x9;&#x9;dgu.Phone = ldapu.Phone,&#xA;&#x9;&#x9;&#x9;&#x9;dgu.Email = ldapu.Email,&#xA;&#x9;&#x9;&#x9;&#x9;dgu.Base = ldapu.Base,&#xA;&#x9;&#x9;&#x9;&#x9;dgu.LastLoadDate = GetDate()&#xA;WHEN NOT MATCHED by Target&#xA;    THEN insert (Username,EmployeeID,AccountName,DisplayName,FullName,FirstName,LastName,Department,Title,Phone,Email,Base,LastLoadDate)&#xA;         values (ldapu.AccountName,ldapu.EmployeeID,ldapu.AccountName,ldapu.DisplayName,ldapu.FullName,ldapu.FirstName,ldapu.LastName,ldapu.Department,ldapu.Title,ldapu.Phone,ldapu.Email,ldapu.Base,GetDate())&#xA;;'&#xA;;&#xA;&#xA;exec sp_executesql @SQL;&#xA;&#xA;-- update groups - ad new groups&#xA;select @sql = '&#xA;MERGE ' + @DG_DB + '.dbo.UserGroups g &#xA;USING (&#xA;&#x9;&#x9;select&#xA;&#x9;&#x9;&#x9;concat(isNull(nullif(lg.GroupName,''''),lg.AccountName), '' (group)'') groupname&#xA;&#x9;&#x9;&#x9;, lg.groupemail&#xA;&#x9;&#x9;&#x9;, lg.accountname&#xA;&#x9;&#x9;&#x9;, lg.grouptype&#xA;&#x9;&#x9;&#x9;, null as EpicId&#xA;&#x9;&#x9;&#x9;, ''LDAP'' as groupsource&#xA;&#x9;&#x9;&#x9;from LDAP.dbo.Groups lg&#xA;&#xA;&#x9;&#x9;&#x9;union all&#xA;&#x9;&#x9;&#x9;&#xA;&#x9;&#x9;&#x9;select distinct GroupName&#xA;&#x9;&#x9;&#x9;, null&#xA;&#x9;&#x9;&#x9;, null&#xA;&#x9;&#x9;&#x9;, GroupSource as grouptype&#xA;&#x9;&#x9;&#x9;, GroupId as EpicId&#xA;&#x9;&#x9;&#x9;, ''Clarity'' as GroupSource &#xA;&#x9;&#x9;&#x9;from ' + @DG_STAGE_DB + '.raw.[clarity-user-groups]&#xA;&#x9;&#x9;) s&#xA;&#xA;&#x9;ON  isnull(g.GroupName, ''asdf'') = isnull(s.groupname,''asdf'')&#xA;&#x9;and isnull(g.GroupEmail,''asdf'') = isnull(s.GroupEmail,''asdf'')&#xA;    and isnull(g.AccountName,''asdf'') = isnull(s.AccountName,''asdf'')&#xA;&#x9;and isnull(g.GroupType,''asdf'') = isnull(s.GroupType,''asdf'')&#xA;&#x9;and isnull(g.EpicId, ''asdf'') = isnull(s.EpicId, ''asdf'')&#xA;WHEN NOT MATCHED by Target&#xA;    THEN insert (AccountName,GroupName,GroupEmail,GroupType,GroupSource,LastLoadDate,EpicId)&#xA;         values (s.AccountName,s.GroupName,s.GroupEmail,s.GroupType,s.groupsource,GetDate(),s.EpicId)&#xA;;';&#xA;&#xA;exec sp_executesql @SQL;&#xA;&#xA;-- LDAP and Epic group membership links&#xA;select @sql = 'merge ' + @DG_DB + '.dbo.UserGroupsMembership m &#xA;&#x9;using (&#xA;&#x9;&#x9;select u.UserId, g.groupId&#xA;&#x9;&#x9;from LDAP.dbo.Memberships m&#xA;&#x9;&#x9;inner join ' + @DG_DB + '.dbo.[User] u on m.AccountName = u.username&#xA;&#x9;&#x9;inner join ' + @DG_DB + '.dbo.UserGroups g on concat(m.GroupName, '' (group)'') = g.GroupName&#xA;&#x9;&#x9;&#xA;&#x9;&#x9;union all&#xA;&#x9;&#x9;&#xA;&#x9;&#x9;select u.userid,g.GroupId&#xA;&#x9;&#x9;from  ' + @DG_STAGE_DB + '.raw.[clarity-user-groups] s&#xA;&#x9;&#x9;join ' + @DG_DB + '.dbo.[User] u on s.user_id = u.EpicId&#xA;&#x9;&#x9;join ' + @DG_DB + '.dbo.userGroups g on g.GroupName = s.GroupName and &#xA;&#x9;&#x9;&#x9;g.GroupType = s.GroupSource and &#xA;&#x9;&#x9;&#x9;g.GroupSource = ''Clarity'' and&#xA;&#x9;&#x9;&#x9;s.groupName is not null&#xA;&#x9;&#x9;) g&#xA;&#x9; on m.Userid = g.UserId&#xA;&#x9; and m.GroupId = g.GroupId&#xA;when not matched by target&#xA;&#x9;then insert (UserId, GroupId, LastLoadDate)&#xA;&#x9;&#x9; values (g.UserId, g.GroupId, GetDate())&#xA;when not matched by source&#xA;&#x9;then delete;&#xA;&#x9;';&#xA;&#xA;exec sp_executesql @SQL;&#xA;&#xA;/*&#xA;-- update LDAP groups - remove unused groups&#xA;select @sql = '&#xA;MERGE ' + @DG_DB + '.dbo.UserGroups dgg USING LDAP.dbo.Groups ldapg&#xA;&#x9;ON dgg.GroupName = concat(isNull(nullif(ldapg.GroupName,''''),ldapg.AccountName), '' (group)'')&#xA;&#x9;and dgg.GroupEmail = ldapg.GroupEmail&#xA;    and dgg.AccountName = ldapg.AccountName&#xA;&#x9;and dgg.GroupType = ldapg.GroupType&#xA;--WHEN NOT MATCHED by Target&#xA;--    THEN insert (AccountName,GroupName,GroupEmail,GroupType)&#xA;--            values (ldapg.AccountName,concat(isNull(nullif(ldapg.GroupName,''''),ldapg.AccountName), '' (group)''),ldapg.GroupEmail,ldapg.GroupType)&#xA;-- cannot delete until membersips are removed. so will delete after&#xA;WHEN NOT MATCHED BY SOURCE&#xA;    THEN DELETE;';&#xA;&#xA;exec sp_executesql @SQL;&#xA;*/&#xA;&#xA;-- ssrs users&#xA;SELECT @SQL = 'use ' + @DG_DB + ';&#xA;-- add SSRS report users&#xA;insert into dbo.[User] (username,LastLoadDate)&#xA;&#xA;select Username, GetDate() from (&#xA;&#x9;select username from ' + @DG_STAGE_DB + '.raw.[report_server_1-reportserver-dbo-users]&#xA;&#x9;union -- not union all. union removes dups&#xA;&#x9;select username from ' + @DG_STAGE_DB + '.raw.[clarity_server-reportserver-dbo-users]&#xA;&#x9;union -- not union all. union removes dups&#xA;&#x9;select username from ' + @DG_STAGE_DB + '.raw.[atlas_db_server-reportserver-dbo-users]&#xA;) as t&#xA;&#xA;where not exists (select * from dbo.[user] as u where u.username = t.username)&#xA;and t.username is not null&#xA;and t.username != ''''&#xA;'&#xA;&#xA;exec sp_executesql @SQL&#xA;&#xA;-- add Epic users&#xA;SELECT @SQL = 'use ' + @DG_DB + ';&#xA;insert into dbo.[user] (Username, DisplayName, LastLoadDate)&#xA;select case when l.domainname is null or l.domainname='''' then l.name&#xA;else concat(''' + @ORG_AD_NAME + '\'', l.domainname) end as Username,&#xA;&#x9;l.name as DisplayName,&#xA;&#x9;GetDate()&#xA;from ' + @DG_STAGE_DB + '.raw.Clarity_Username_Domainname_Links l&#xA;where not exists ( select * from dbo.[user] as u where replace(u.username,''' + @ORG_AD_NAME + '\'','''') = isnull(nullif(l.domainname,''''),l.name))&#xA;'&#xA;&#xA;exec sp_executesql @SQL&#xA;&#xA;-- add Epic ID to all users&#xA;SELECT @SQL = 'use ' + @DG_DB + ';&#xA;update dbo.[user]&#xA;set &#xA;&#x9;&#x9;dbo.[user].EpicId = l.User_id,&#xA;&#x9;&#x9;dbo.[user].LastLoadDate = GetDate()&#xA;&#x9;&#xA;from&#xA;dbo.[user] u&#xA;join ' + @DG_STAGE_DB + '.raw.Clarity_Username_Domainname_Links l on replace(u.username,''' + @ORG_AD_NAME + '\'','''') = isnull(nullif(l.domainname,''''),l.name)&#xA;where u.EpicId &lt;&gt; l.User_id&#xA;'&#xA;&#xA;exec sp_executesql @SQL&#xA;&#xA;" xmlns:SQLTask="www.microsoft.com/sqlserver/dts/tasks/sqltask">
          <SQLTask:ParameterBinding
            SQLTask:ParameterName="0"
            SQLTask:DtsVariableName="$Package::Data_Governance_InitialCatalog"
            SQLTask:ParameterDirection="Input"
            SQLTask:DataType="130"
            SQLTask:ParameterSize="-1" />
          <SQLTask:ParameterBinding
            SQLTask:ParameterName="1"
            SQLTask:DtsVariableName="$Package::DG_Staging_InitialCatalog"
            SQLTask:ParameterDirection="Input"
            SQLTask:DataType="130"
            SQLTask:ParameterSize="-1" />
          <SQLTask:ParameterBinding
