select          N'clarity_server'                                                             as SourceServer
,               N'clarityreport'                                                          as SourceDB
,               N'report_info'                                                            as SourceTable
,               convert(nvarchar(max), hrx.REPORT_INFO_NAME)                              as Name
,               ltrim(stuff((
                                select   N' ' + d.REPORT_DESCRIPTION
                                from     raw.[clarity_server-clarityreport-dbo-report_desc] d
                                where    d.REPORT_INFO_ID = hrx.REPORT_INFO_ID
                                order by d.REPORT_INFO_ID
                                ,        d.LINE
                                for xml path('')
                            )
                      ,     1
                      ,     1
                      ,     ''
                      )
                )                                                                         as Description
,               null                                                                      as DetailedDescription
,               case
                    when rec_type.NAME = N'Settings' then N'Application Report'
                    when rec_type.NAME = N'Workbench' then N'Reporting Workbench Report'
                    when rec_type.NAME is null then N'Other HRX Report'
                    else rec_type.NAME + N' Report'
                end                                                                       as ReportObjectType
,               case when author.domainname is null then author.name else concat(N'',?,'\', author.domainname) end as Author
,               case when modder.domainname is null then modder.name else concat(N'',?,'\', modder.domainname) end as LastModifiedBy
,               hrx.INST_OF_LAST_MOD_DTTM                                                 as LastModifiedDate
,              concat(N'',hrg_2.crystal_filename)                                                   as ReportObjectURL
,               N'HRX'                                                                    as EpicMasterFile
,               hrx.REPORT_INFO_ID                                                        as EpicRecordID
,       null                                    as ReportServerCatalogID
,       case
          when untested_hrx.report_id is null --Not a zzz-untested hrx security override 
              and (private_or_public_c &lt;&gt; 1 or private_or_public_c is null) --not set to private
              and inactive_untested_templates.report_id is null --not living on a zzz-untested or inactive template   
            then N'Y'
          else N'N'
        end                                   
                                              as DefaultVisibilityYN
,       hrx.REPORT_ID                               as ReportTemplateId

from            [raw].[clarity_server-clarityreport-dbo-report_info]      hrx
left outer join raw.[clarity_server-clarityreport-dbo-zc_report_type_hgr] rec_type on hrx.RECORD_TYPE_C = rec_type.REPORT_TYPE_HGR_C
left outer join raw.Clarity_Username_Domainname_Links        author on author.USER_ID = hrx.CREATED_BY_USER_ID
--left outer join raw.[clarity_server-clarityreport-dbo-clarity_emp]        author on author.USER_ID = hrx.CREATED_BY_USER_ID
left outer join raw.[clarity_server-clarityreport-dbo-emp_basic_info]     author2 on author2.USER_ID = author.USER_ID
left outer join raw.Clarity_Username_Domainname_Links        modder on modder.USER_ID = hrx.LAST_MOD_BY_USER_ID
--left outer join raw.[clarity_server-clarityreport-dbo-clarity_emp]        modder on modder.USER_ID = hrx.LAST_MOD_BY_USER_ID
left outer join raw.[clarity_server-clarityreport-dbo-emp_basic_info]     modder2 on modder2.USER_ID = modder.USER_ID
left outer join raw.[clarity_server-clarityreport-dbo-template_info] hrg on hrg.REPORT_ID = hrx.REPORT_ID
left outer join raw.[clarity_server-clarityreport-dbo-template_info_2] hrg_2 on hrg_2.report_id = hrg.REPORT_ID
left outer join 
(
  select distinct report_id 
  from raw.[clarity_server-clarityreport-dbo-ovride_rpt_groups]
  where report_group_c = '308000001' --zzz-untested
)                                   as untested_hrx on untested_hrx.report_id = hrx.report_info_id
left outer join 
(
  select distinct report_id 
  from raw.[clarity_server-clarityreport-dbo-template_info] template_info
    left join 
    (
      select distinct assoc_report_id temp_id 
      from raw.[clarity_server-clarityreport-dbo-clarity_rpt_groups] g 
        inner join raw.[clarity_server-clarityreport-dbo-clarity_rpt] rpt on rpt.report_id = g.report_id 
      where report_group_c = '308000001' --zzz-untested
    ) f on f.temp_id = template_info.report_id

  where f.temp_id is not null--living on a zzz-untested report template
    or status_c &lt;&gt; 0  --not active status
)                                   as inactive_untested_templates on inactive_untested_templates.report_id=hrx.report_id

where hrx.TEMP_REPORT_C = 0 --Active only. Dont want temp hrx's anywhere, not even with no visibility. They are not the droids we're looking for.</property>
