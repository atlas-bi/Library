select          N'clarity_server'                                       as SourceServer
,               N'clarityreport'                                    as SourceDB
,               N'dashboard_info'                                   as SourceTable
,               coalesce(idmt.NAME, N'Other') + N' Radar Dashboard' as ReportObjectType
,               convert(nvarchar(max), idm.DASHBOARD_NAME)          as Name
,               convert(nvarchar(max), idm.display_title)           as DisplayTitle
-- Description table has multiple lines per HRX. We must use XML to concatenate them.
,               ltrim(stuff((
                                select   N' ' + d.RECORD_DESC
                                from     raw.[clarity_server-clarityreport-dbo-dashboard_desc] d
                                where    d.DASHBOARD_ID = idm.DASHBOARD_ID
                                order by d.DASHBOARD_ID
                                ,        d.LINE
                                for xml path('')
                            )
                      ,     1
                      ,     1
                      ,     ''
                      )
                )                                                   as Description
,               null                                                as DetailedDescription
,               case when puser.domainname is null then puser.name else concat(N'',?,'\', puser.domainname) end                      as Author
,               null                                                as LastModifiedBy
,               idm.INSTANT_OF_UPD_DTTM                             as LastModifiedDate
,       null                        as ReportObjectURL
,               N'IDM'                                              as EpicMasterFile
,               idm.DASHBOARD_ID                                    as EpicRecordID
,       null                        as ReportServerCatalogID
,       case
          when idm.RECORD_TYPE_C &lt;&gt; 3 --not personal
              and coalesce(idm_override.READY_FOR_USE_YN,idm.READY_FOR_USE_YN) = 'Y' --check override record first if it exists, needs to be marked ready for use
                      and coalesce(idm_override.ENABLED_YN,idm.ENABLED_YN) = 'Y' --check override record first if it exists, needs to be marked enabled
              and idm.RECORD_STATUS_C is null --not hidden or deleted
            then N'Y'
          else N'N'
        end                         as DefaultVisibilityYN

