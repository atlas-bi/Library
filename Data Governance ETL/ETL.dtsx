select ParentReportObjectBizKey
, ChildReportObjectBizKey
, ROW_NUMBER() OVER(PARTITION BY ParentReportObjectBizKey ORDER BY REGION ASC, LINE ASC) LINE 

from -- get list of all db/comp relationships and sort components based on first appearance on db
(
  select distinct     
    ParentReportObjectBizKey = concat(
                    N'clarity_server'  
                  ,   '||'
                  ,   N'clarityreport'  
                  ,   '||'
                  ,   coalesce(idmt.NAME, N'Other') + N' Radar Dashboard'
                  ,   '||'
                  ,   null
                  ,   '||'
                  ,   N'IDM' 
                  ,   '||'
                  ,   idm.DASHBOARD_ID
                  )

  , ChildReportObjectBizKey = concat(
                    N'clarity_server'  
                  ,   '||'
                  ,   N'clarityreport'  
                  ,   '||'
                  ,   concat(coalesce(idbt.NAME, N'Other'), N' Radar Dashboard Component')
                  ,   '||'
                  ,   null
                  ,   '||'
                  ,   N'IDB' 
                  ,   '||'
                  ,   idb.COMPONENT_ID
                  )   
                            
  , idbl.REGION
  , idbl.LINE 
  , ROW_NUMBER() OVER(PARTITION BY idm.DASHBOARD_ID, idb.COMPONENT_ID ORDER BY REGION ASC, LINE ASC) IDB_LINE


  from            raw.[clarity_server-clarityreport-dbo-dashboard_info]    idm
  left outer join raw.[clarity_server-clarityreport-dbo-zc_record_type_24] idmt on idmt.RECORD_TYPE_24_C = idm.RECORD_TYPE_C
  left outer join raw.[clarity_server-clarityreport-dbo-component-list]  idbl on idbl.DASHBOARD_ID = idm.DASHBOARD_ID
  inner join raw.[clarity_server-clarityreport-dbo-component_info]    idb  on idb.COMPONENT_ID = idbl.COMPONENT_ID
  left outer join raw.[clarity_server-clarityreport-dbo-zc_record_type_24] idbt on idbt.RECORD_TYPE_24_C = idb.RECORD_TYPE_C

  where idbl.COMPONENT_ID is not null
) db_comps

where IDB_LINE = 1 --only want idm/idk relationship once, use the first time it appears on the db</property>
